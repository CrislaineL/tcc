datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  senha String
}

model Consulta{
  id Int @id @default(autoincrement())
  nomepet String
  especie Especie
  raca String?
  nomeproprietario String
  datanascpet DateTime
  email String @unique
  alergia String?
}

enum Especie {
  cachorro
  gato
  peixe
  passaro
}